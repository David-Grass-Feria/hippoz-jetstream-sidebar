{
    "name": "hippoz/hippoz-jetstream-sidebar",
    "description": "This package replace the layout with a sidebar.",
    "license": "MIT",
    "authors": [
        {
            "name": "David Grass Feria",
            "email": "contact@hippoz.org",
            "homepage": "https://hippoz.org"
        }
    ],
    "homepage": "https://github.com/hippoz/hippoz-jetstream-sidebar",
    "keywords": ["Laravel", "HippozJetstreamSidebar"],
    "require": {
        "illuminate/support": "~9",
        "laravel/jetstream": "^2.12.3"
    },
    "require-dev": {
        "phpunit/phpunit": "~9.0",
        "orchestra/testbench": "~7"
    },
    "autoload": {
        "psr-4": {
            "Hippoz\\HippozJetstreamSidebar\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Hippoz\\HippozJetstreamSidebar\\Tests\\": "tests"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Hippoz\\HippozJetstreamSidebar\\HippozJetstreamSidebarServiceProvider"
            ],
            "aliases": {
                "HippozJetstreamSidebar": "Hippoz\\HippozJetstreamSidebar\\Facades\\HippozJetstreamSidebar"
            }
        }
    }
}
